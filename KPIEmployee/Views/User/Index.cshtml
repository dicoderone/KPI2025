            <a asp-action="ChatBlock" id="show_chat_block" class="btn btn-app btn-info out_to_chat">
                <span id="chat_badge" class="badge bg-red" hidden="@(ViewBag.notReadedMsg > 0 ? false : true)">@(ViewBag.notReadedMsg > 0 ? "" + ViewBag.notReadedMsg : "")</span>
                <i class="fa fa-wechat"></i> Chat
            </a>
            <!-- /Content Chat -->
