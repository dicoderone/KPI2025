            <!-- Modal content-->
            <div class="modal-content">
                <form asp-action="SaveINN" method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Пожалуйста, введите ИНН</h4>
                    </div>
                    <div class="modal-body">
                        <input name='id' type=hidden value='@ViewBag.user_id'>
                        <input name='inn' class='form-control w-400' />
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        <button type="submit" name="submit" class="btn btn-bold btn-pure btn-primary float-right">Отправить</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- ./modal -->
    <!-- Modal PIN-->
    <div class="modal fade" id="myModalPin" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <form asp-action="SavePIN" method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
