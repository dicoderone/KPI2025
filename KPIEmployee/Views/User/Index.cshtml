                    </li>

                </ul>
            </section>
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div class="box" @((ViewBag.user_inn == null || ViewBag.user_inn == "" || ViewBag.user_pin == null || ViewBag.user_pin == "") ? "" : " hidden='hidden' ")>
                <div class="box-header with-border" @((ViewBag.user_inn == null || ViewBag.user_inn == "") ? "" : " hidden='hidden' ")>
                    <p class="box-title" style="font-weight: bold; color: #FF0000;">Чтобы активировать ваш профиль, пожалуйста, введите <a class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModalInn">ИНН</a></p>
                </div>
                <div class="box-header with-border" @((ViewBag.user_pin == null || ViewBag.user_pin == "") ? "" : " hidden='hidden' ")>
                    <p class="box-title" style="font-weight: bold; color: #FF0000;">Чтобы активировать ваш профиль, пожалуйста, введите <a class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModalPin">ПИНФЛ</a></p>
                </div>
            </div>
            @if (ViewBag.pinmessages != null && ViewBag.pinmessages != "")
            {
                <text>
                    <div class="box">
                        <div class="box-header with-border">
                            <p class="box-title" style="font-weight: bold; color: #FF0000;"> @Html.Raw("" + ViewBag.pinmessages) </p>
                        </div>
                    </div>
                </text>
            }

            <!-- Main content -->
            <section class="content" style="padding: 15px;" id="pustoy">
                @RenderBody()
            </section>
            <!-- /.content -->
