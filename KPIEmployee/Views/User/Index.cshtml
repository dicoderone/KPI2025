                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Пожалуйста, введите ПИНФЛ</h4>
                    </div>
                    <div class="modal-body">
                        <input name='id' type=hidden value='@ViewBag.user_id'>
                        <input name='pin' class='form-control w-400' />
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        <button type="submit" name="submit" class="btn btn-bold btn-pure btn-primary float-right">Отправить</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- ./modal -->
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    

    <style type="text/css">
        .error {
            color: red;
        }
    </style>

    @RenderSection("scripts", required: false)

